{
  "manifest_version": 3,
  "name": "YouTube 视频加速",
  "version": "1.0",
  "description": "为YouTube视频添加更多倍速选择",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "/images/favicon_48x48.png",
    "96": "/images/favicon_96x96.png",
    "144": "/images/favicon_144x144.png"
  }
}
